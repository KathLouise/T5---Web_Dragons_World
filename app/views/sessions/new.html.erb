<main>
	<article class="information">
		<h2 class="subtitle">Log In</h2>
		<%= form_tag sessions_path do %>
			<div class="field">
				<%= label_tag :username %><br>
				<%= text_field_tag :username %>
			</div>
  			<div class="field">
    				<%= label_tag :password %><br>
				<%= password_field_tag :password %>
			</div>
  			<div class="actions">
    				<%= submit_tag "Log In" %>
			</div>
		<% end %>
	</article>
